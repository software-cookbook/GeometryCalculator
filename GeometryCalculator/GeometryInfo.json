{
  "shapes": [
    {
      "name": "rectangle",
      "parameters": [ "length", "width" ],
      "areaFormula": "length * width"
    },
    {
      "name": "circle",
      "parameters": [ "radius" ],
      "areaFormula": "3.14159 * radius * radius"
    },
    {
      "name": "triangle",
      "parameters": [ "base", "height" ],
      "areaFormula": "0.5 * base * height"
    },
    {
      "name": "parallelogram",
      "parameters": [ "base", "height" ],
      "areaFormula": "base * height"
    },
    {
      "name": "trapezoid",
      "parameters": [ "base1", "base2", "height" ],
      "areaFormula": "0.5 * (base1 + base2) * height"
    },
    {
      "name": "ellipse",
      "parameters": [ "majorAxis", "minorAxis" ],
      "areaFormula": "3.14159 * majorAxis * minorAxis"
    }
  ]
}
